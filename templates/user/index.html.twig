{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}
<div class="users">
    <h3>Barre de recherche</h3>
    <table>
        <tr>
            <th>Nom</th>
            <th>Prénom</th>
            <th>Fonction</th>
            <th>Formation</th>
            <th>Activité</th>
            <th>Action</th>
        </tr>
        <tr>
            {% for user in users %}
                <th>{{ user.lastname}}</th>
            {% endfor %}
        </tr>
    </table>
</div>
{% endblock %}
